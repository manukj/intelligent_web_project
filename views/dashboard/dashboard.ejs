<!DOCTYPE html>
<html lang="en" data-theme="cupcake">
<head>
  <title>Plant Sightings Dashboard</title>
  <link href="/css/output.css" rel="stylesheet" />
  <link rel="manifest" href="/manifest.json" />
  <script src="/javascripts/dashboard_script.js"></script>
</head>
<body>
<div class="flex flex-col h-screen">

  <div >
    <%- include('./header_components.ejs') %>
  </div>


  <div class="grid grid-cols-1 sm:grid-cols-3 gap-4 p-4 mt-20">
    <% plantData.forEach(function(plant) { %> <%-
    include('./plant_item_component.ejs', { plant: plant }) %> <% }) %>
  </div>

  <button class="btn btn-neutral shadow-2xl btn-lg fixed bottom-10 right-10 text-4xl text-white" onclick="openAddPlantPage()">
    + <div class="text-lg">Add Plant</div>
  </button>

</div>
<%- include('../components/online_offline_mode.ejs') %>
</body>
</html>